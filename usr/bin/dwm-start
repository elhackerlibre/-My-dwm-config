#!/bin/bash

# setting keuboard layout /usr/share/X11/xkb/rules/base.lst
setxkbmap us -variant intl

xset +fp /usr/share/fonts/local
xset fp rehash

insync start &
parcellite &
mpd &

#Set status bar & start DWM
dwm-status &

# Background
feh --bg-fill  /home/virus69/Im√°genes/dwm/dwm.png

make -C /home/virus69/dwm
exec /home/virus69/dwm/dwm
#exec dwm
exec numlockx on
